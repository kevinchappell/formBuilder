{
  "name": "formbuilder-languages",
  "version": "1.1.0",
  "description": "Language files for formBuilder",
  "main": "index.js",
  "files": [
    "*.lang",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint index.js",
    "semantic-release": "semantic-release",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinchappell/formBuilder-languages.git"
  },
  "keywords": [
    "formBuilder",
    "lang",
    "files",
    "i18n"
  ],
  "author": "Kevin Chappell <kevin.b.chappell@gmail.com> (http://kevin-chappell.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinchappell/formBuilder-languages/issues"
  },
  "homepage": "https://github.com/kevinchappell/formBuilder-languages#readme",
  "dependencies": {
    "mi18n": "^0.4.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-angular": "^7.6.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.9",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.1.4",
    "semantic-release": "^15.13.12"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  }
}
