{
  "name": "formBuilder",
  "version": "1.24.6",
  "main": "dist/form-builder.js",
  "files": [
    "dist/form-builder.js",
    "dist/form-builder.min.js",
    "dist/form-builder.css",
    "dist/form-builder.min.css",
    "dist/form-render.css",
    "dist/form-render.js",
    "dist/form-render.min.css",
    "dist/form-render.min.js"
  ],
  "homepage": "https://formbuilder.online/",
  "repository": {
    "url": "http://github.com/kevinchappell/formBuilder",
    "type": "git"
  },
  "author": "Kevin Chappell <kevin.b.chappell@gmail.com>",
  "contributors": [],
  "bugs": "https://github.com/kevinchappell/formBuilder/issues",
  "description": "A jQuery plugin for drag and drop form building",
  "keywords": [
    "jquery-plugin",
    "forms",
    "drag and drop",
    "form builder",
    "form create"
  ],
  "license": "MIT",
  "ignore": [
    "**/*",
    "node_modules",
    "test"
  ],
  "config": {
    "files": {
      "test": [
        "test/**/*.spec.js"
      ],
      "formBuilder": {
        "js": [
          "src/js/polyfills.js",
          "src/js/utils.js",
          "src/js/helpers.js",
          "src/js/events.js",
          "src/js/kc-toggle.js",
          "src/js/form-builder.js"
        ],
        "fonts": "src/fonts/fontello",
        "sass": [
          "src/sass/form-builder.scss"
        ]
      },
      "formRender": {
        "js": [
          "src/js/polyfills.js",
          "src/js/kc-toggle.js",
          "src/js/utils.js",
          "src/js/form-render.js"
        ],
        "sass": [
          "src/sass/form-render.scss"
        ]
      },
      "site": [
        "demo/assets/sass/site.scss"
      ]
    },
    "fontServer": "http://fontello.com"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "prestart": "npm update",
    "test": "gulp test"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.11.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-base64": "^0.1.3",
    "gulp-bump": "^2.4.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-eslint": "^3.0.1",
    "gulp-filter": "^4.0.0",
    "gulp-git": "^1.7.0",
    "gulp-header": "^1.7.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "opener": "^1.4.2",
    "semver": "^5.3.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "globals": {
      "jQuery": true,
      "$": true
    },
    "rules": {
      "no-console": 0,
      "new-cap": 0,
      "strict": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "engines": {}
}
