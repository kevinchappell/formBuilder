        "js": [
          "src/js/fields.js",
          "src/js/field.js",
          "src/js/htmlentities.js",
          "src/js/helpers.js",
          "src/js/events.js",
          "src/js/kc-toggle.js",
          "src/js/form-builder.js",
          "src/js/to-xml.js",
          "src/js/polyfills.js"
